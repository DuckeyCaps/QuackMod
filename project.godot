; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="QuackMod"
config/description="Time to get quackin'!"
config/version="2.0"
run/main_scene="res://main_program.tscn"
config/features=PackedStringArray("4.2", "C#", "GL Compatibility")
boot_splash/image="res://Assets/SplashScreen.png"
config/icon="res://Assets/duck.png"

[display]

window/size/borderless=true
window/energy_saving/keep_screen_on=false
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="SoundBinder"

[filesystem]

import/blender/enabled=false

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
